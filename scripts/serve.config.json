{
    "headers": [
        {
            "source": "manifest.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=7200, stale-while-revalidate=86400"
                }
            ]
        },
        {
            "source": "**/*.png",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=7200, stale-while-revalidate=86400"
                }
            ]
        },
        {
            "source": "*.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31540000, immutable"
                }
            ]
        }
    ]
}
